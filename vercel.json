{
  "version": 2,
  "builds": [
    {
      "src": "trello-clone-api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "trello-clone-client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/trello-clone-api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/trello-clone-client/dist/$1"
    }
  ]
}